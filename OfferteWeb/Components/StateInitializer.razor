@using OfferteWeb.Handler
@inject AppState state;
@code {
    // [Parameter]
    // public InitialApplicationState InitialAppState { get; set; }
    [Parameter]
    public Action StateInitialized { get; set; }

    protected override void OnInitialized()
    {
        base.OnInitialized();
        state.Initialize();
        StateInitialized?.Invoke();
    }

}
